{
  "expo": {
    "name": "PB",
    "slug": "plannerbuddy",
    "description": "Streamline Your Guest's and Event's Details With Planner Buddy",
    "scheme": "com.vitabletech.plannerbuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "owner": "Vitabletech",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vitabletech.plannerbuddy"
    },
    "android": {
      "userInterfaceStyle": "automatic",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.vitabletech.plannerbuddy",
      "versionCode": 1
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-contacts"
    ]
  }
}
